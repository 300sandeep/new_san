	<%=render 'layouts/clinic_client_sidebar'%>
	<div class="db-mid-3">
		<div class="db-grid">
			<div class="md-block text-center">
				<button class="fullbody-btn">Full Body</button>
			</div>
			<div class="d-flex justify-content-between flex-md-column flex-lg-column flex-xl-row flex-nowrap mt-40 ind-res">
				<div class="align-self-start ind-left">
					<ul class="b-tabs">
						<li class="tab-link an-link1">
							<span>Anterior Neck</span>
						</li>
						<li class="tab-link fc-link2">
							<span>Front Chest</span>
						</li>
						<li class="tab-link fc-link2">
							<span>Biceps</span>
						</li>
						<li class="tab-link hn-link-3">
							<span>Hands/Forearms</span>
