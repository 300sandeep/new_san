<div class="table-responsive ac-right">
	<table class="table" cellpadding="0" cellspacing="0">
    <thead>
      <tr>
        <th><%= sort_link "first_name"%>
        	<span class="d-arrow">
        		<img src="/assets/dropdown.png" alt="image">
        	</span>
        </th>
        <th>Active / Inactive
        	<span class="d-arrow">
        		<img src="/assets/dropdown.png" alt="image">
        	</span>
        </th>
        <th>Last Session
        	<span class="d-arrow">
        		<img src="/assets/dropdown.png" alt="image">
        	</span>
        </th>
      </tr>
    </thead>
    <tbody>
      <%@clients.each do |c|%>
        <tr>
          <td class="g-color"><%=link_to "#{c.first_name}", rehab_clinic_clients_path%></td>
          <td class="db-txt">Active</td>
          <td class="db-txt">12/29/2017</td>
        </tr>
      <%end%>
    </tbody>
	</table>
</div>

<%= paginate @clients%>

